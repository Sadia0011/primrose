    "section_rich_text_GMbFPX": {
      "type": "section-rich-text",
      "blocks": {
        "heading_UbPzcy": {
          "type": "heading",
          "settings": {
            "title": "Plants Terms and Conditions",
            "heading_font_size": "heading-medium",
            "padding_bottom": 16,
